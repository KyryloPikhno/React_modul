{
  "extends": ["react-app", "plugin:prettier/recommended"],
  "plugins": ["simple-import-sort", "sort-keys-fix"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "simple-import-sort/imports": "error",
    "sort-keys-fix/sort-keys-fix": "warn"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
